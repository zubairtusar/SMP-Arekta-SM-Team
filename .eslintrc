{
    "parser": "babel-eslint",
    "rules": {
        "eqeqeq": "error", //done
        "block-scoped-var": "error", //done
        "comma-spacing": ["error", { "before": false, "after": true }],
        "complexity": [
            "error",
            {
                "max": 0
            }
        ], //done
        "no-empty-function": "error", //done
        "no-empty-pattern": "error", //done
        "no-useless-return": "error", //done
        "no-unused-expressions": "error", //done
        "max-depth": [
            "error",
            {
                "max": 1
            }
        ], //done
        "max-lines": [
            "error",
            {
                "max": 0
            }
        ], //done
        // new ones
        "curly": [
            "error",
            "all"
        ],
        "id-length": [
            "error",
            {
                "max": 0
            }
        ], //done
        "max-nested-callbacks": [
            "error",
            {
                "max": 0
            }
        ],
        "brace-style": ["error", "1tbs"],
        "no-alert": "error",
        "no-console": "error", //done
        "no-lone-blocks": "error",
        "no-loop-func": "error",
        "no-param-reassign": "error",
        "no-shadow": "error",
        "no-unused-return": "error",
        "array-bracket-newline": "error",
        "indent": [
            "error",
            2
        ],
        "no-extra-parens": [
            "error",
            "all"
        ],
        "no-mixed-spaces-and-tabs": "error",
        "no-multi-spaces": "error",
        "no-trailing-spaces": "error",
        "no-whitespace-before-property": "error"
    }
}